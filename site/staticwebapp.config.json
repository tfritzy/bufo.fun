{
  "globalHeaders": {
    "cache-control": "public, max-age=604800"
  },
  "routes": [
    {
      "route": "/bufo-data.json",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/bufo-data.json", "/bufos/*", "/smolBufos/*", "/*.{js,css,png,jpg,jpeg,gif,svg,ico,json,woff,woff2,ttf,eot}"]
  }
}
